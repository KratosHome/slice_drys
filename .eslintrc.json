{
  "root": true,
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "next/typescript"
  ],
  "rules": {
    "prettier/prettier": [
      "off",
      {
        "endOfLine": "auto"
      }
    ],
    "no-console": [
      "error",
      {
        "allow": ["error", "warn"]
      }
    ]
  }
}
